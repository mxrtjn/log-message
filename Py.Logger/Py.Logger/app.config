<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
    <connectionStrings>
        <add
         name="log" 
         connectionString="Server=localhost;Database=log;User ID=logUser;Password=logPassword;"
         providerName="System.Data.SqlClient"/>
    </connectionStrings>
    <appSettings>
        <add key="LogFilePath" value="log.log" />
    </appSettings>
</configuration>
